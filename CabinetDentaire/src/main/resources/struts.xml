<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name = "struts.devMode" value = "true" />
    <package name="CabinetDentaire" extends="struts-default">
        <!-- Add your actions here -->
        <action name="AddEmploye" class="cabinet.actions.EmployeAction" method="execute">
            <result name="success">/jsp/successEmploye.jsp</result>
            <result name="input">/jsp/AddEmploye.jsp</result>
        </action>
         <action name="AddPatient" class="cabinet.actions.PatientAction" method="execute">
            <result name="success">/jsp/success.jsp</result>
            <result name="input">/jsp/AddPatient.jsp</result>
        </action>
        <action name="fetchAll" class="cabinet.actions.PatientAction" method="fetch">
            <result name="fetch">/jsp/tablePatient.jsp</result>
        </action>
    </package>
</struts>